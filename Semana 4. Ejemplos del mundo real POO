class Habitacion:
    def __init__(self, numero, tipo, precio):
        self.numero = numero
        self.tipo = tipo
        self.precio = precio
        self.disponible = True

    def __str__(self):
        return f'Habitaci√≥n {self.numero} ({self.tipo}) - ${self.precio}/noche - {"Disponible" if self.disponible else "No Disponible"}'
